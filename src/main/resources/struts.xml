<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">
 
<struts>
    <constant name="struts.devMode" value="true" />
	<package name="default" extends="struts-default">
		<action name="videos" class="com.swift.example.action.VideoAction">
			<result name="videos">/videos.jsp</result>
		</action>
		<action name="music" class="com.swift.example.action.MusicAction">
		    <result name="music">/music.jsp</result>
		</action>
	</package>
	
	<package name="login" extends="struts-default">
	    <action name="login" class="com.swift.example.action.LoginAction">
	        <result name="success">/index.jsp</result>
	        <result name="failure">/login.jsp</result>
	    </action>
	</package>
 
</struts>